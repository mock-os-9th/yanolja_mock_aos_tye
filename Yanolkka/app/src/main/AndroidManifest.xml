<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yanolkka">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".src.ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".src.search.motel.SearchMotelActivity"></activity>
        <activity android:name=".src.signup.SignUpActivity" />
        <activity android:name=".src.reservation.ReservationActivity" />
        <activity android:name=".src.room.info.RoomInfoActivity" />
        <activity android:name=".src.src.result.SearchResultActivity" />
        <activity android:name=".src.search.SearchActivity" />
        <activity android:name=".src.signin.SignInActivity" />
        <activity android:name=".src.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>